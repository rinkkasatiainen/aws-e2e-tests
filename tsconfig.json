{
  "compilerOptions": {
    "target":"ES2018",
    "module": "commonjs",
    "lib": ["es2016", "es2017.object", "es2017.string"],
    "declaration": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "noFallthroughCasesInSwitch": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization":false,
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "*": [
        "*",
        "node_modules/*"
      ]
    }
  },
  "include": [
    "cdk/bin/cdk.ts"
  ],
  "exclude": [
    "node_modules",
    "cdk.out"
  ]
}
